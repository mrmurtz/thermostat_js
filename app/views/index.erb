<html>
  <head>
    <meta charset="utf-8">
    <title>Thermostat</title>
    <link rel="stylesheet" type="text/css" href="style.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<meta name="viewport" content="width=device-width, initial-scale=1">
  </head>


  <body>

    <div class="container">
    <div class="de">
        <div class="den">
          <div class="dene">
            <div class="denem">
              <div class="deneme">
                <div id = "temperature"></div><strong>&deg;</strong>
              </div>
            </div>
          </div>
        </div>
    </div>
</div>


    <section>
      <div class="container">
      <h1 id="temperature" class="yellow"></h1>
      <p>
        <button class="btn btn-success" id="temperature-down">-</button>
        <button class="btn btn-info" id="temperature-reset">reset</button>
        <button class="btn btn-danger" id="temperature-up">+</button>
        </p>
        <p>
        power saving mode is <span id="power-saving-status">on</span>
      </p>
      <p>
        <button class="btn btn-primary btn-lg active" id="powersaving-on">PSM on</button>
        <button class="btn btn-primary btn-lg" id="powersaving-off">PSM off</button>
      </p>
      </div>
    </section>

    <section>
      <div class="container">
      <h3>Current temperature in <span id="city"></span>: <span id="current-temperature"></span></h3>
        <form id="select-city">
          <input id="current-city" type="text" placeholder="Enter a city"></input>
          <input type="submit"></input>
        </form>
        <div class="container">
    </section>

      <script src="jquery.js"></script>
      <script src="src/Thermostat.js"></script>
      <script src="dashboard.js"></script>
  </body>
</html>
